
<link rel="stylesheet" href="@Url.Content("~/Content/Styles/Profile/ProfileBar.css")" />
<div class="face">
    <div class="img-circle">
        <img class="img-circle" data-bind="attr:{src: Profile.Avatar}" />
    </div>
</div>
<div class="box">
    <div class="box-name">
        <div class="row">
            <div class="col-xs-4"></div>
            <div class="col-xs-8 margin-5"><span data-bind="text: Profile.FirstName"></span></div>
        </div>
    </div>
    <div class="box-div-stat">
        <div class="col-xs-4 box-stat center"><i class="icon-2xx icon-bullseye"><span class="box-stat-icon-text">0</span></i></div>
        <div class="col-xs-4 box-stat center"><i class="icon-2xx icon-comments"><span class="box-stat-icon-text">0</span></i></div>
        <div class="col-xs-4 box-stat center"><i class="icon-2xx icon-heart "><span class="box-stat-icon-text">0</span></i></div>
    </div>
</div>
<script src="@Url.Content("~/Scripts/Profile/ProfileBar.js")" type="text/javascript"></script>