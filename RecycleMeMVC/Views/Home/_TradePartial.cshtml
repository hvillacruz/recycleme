
<link href="~/Content/Styles/Home/default.css" rel="stylesheet" />
<link href="~/Content/Styles/Home/component.css" rel="stylesheet" />
<link href="~/Content/Styles/Home/TradeItems.css" rel="stylesheet" />
<script src="~/Scripts/modernizr.custom.js"></script>



<div id="tradeContainer" class="container">

    <div class="search-box">
        <input id="seachTxtBox" placeholder="Search..."><span class="fa fa-search icon" data-bind="click: Search.bind($data, $('#seachTxtBox').val())"></span>

    </div>


    <ul class="grid effect-2" id="grid" data-bind="foreach: Items">
        <!-- ko if: ItemImages.length > 0 -->

        <li>
            <div class="widget">
                <div class="cover">
                    <img data-bind="attr:{src: ItemImages[0].Path}" />
                </div>

                <h1><span data-bind="text:Name"></span></h1>



                <div class="stat-stat">
                    <div class="col-xs-4 box-stat box-stat-first center radius-round-bottom " @*data-bind="click:  $parent.likeUser"*@><i class="fa icon-2xx fa-heart "><span class="box-stat-icon-text" @*data-bind="text:UserFollowerUsers.length"*@>0</span></i></div>
                    <div class="col-xs-4 box-stat center radius-round-bottom"><i class="fa icon-2xx fa-comments"><span class="box-stat-icon-text" @*data-bind="text:UserCommenter.length"*@>0</span></i></div>
                    <div class="col-xs-4 box-stat center radius-round-bottom"><i class="fa icon-2xx fa-share-alt"><span class="box-stat-icon-text">0</span></i></div>
                </div>

            </div>

        </li>

        <!-- /ko -->
    </ul>
</div>
<script src="~/Scripts/Home/TradeItems.js"></script>



<script src="~/Scripts/masonry.pkgd.min.js"></script>
<script src="~/Scripts/imagesloaded.js"></script>
<script src="~/Scripts/classie.js"></script>


<script src="~/Scripts/AnimOnScroll.js"></script>
