            <div class="navbar-header pull-left">
                <a href="#" class="navbar-brand">
                    <small>
                        <i class="icon-beer"></i>
                        Recycle Me
                    </small>
                </a><!-- /.brand -->
            </div><!-- /.navbar-header -->

