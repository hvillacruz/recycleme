<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Recycle Me</title>

    <meta name="description" content="overview &amp; stats" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @Styles.Render("~/Content/Assets/Recycle")
    @Styles.Render("~/Content/Styles/Recycle")
    @Scripts.Render("~/bundles/modernizr")


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @*@Scripts.Render("~/bundles/knockout-kendo")*@
    @Scripts.Render("~/bundles/knockout")

    <script src="@Url.Content("~/Scripts/string.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/datajs-1.1.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Main/Global.js")" type="text/javascript"></script>
